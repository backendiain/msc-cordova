<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.cordova.plugin.trigger.benchmark" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>cordovaPluginTriggerBenchmark</name>

	<platform name="android">
        <js-module src="www/android/cordovaPluginTriggerBenchmark.js" name="cordovaPluginTriggerBenchmark">
            <runs/>
            <clobbers target="cordova.plugins.cordovaPluginTriggerBenchmark" />
        </js-module>

		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="cordovaPluginTriggerBenchmark">
				<param name="android-package" value="com.cordova.plugin.trigger.benchmark.cordovaPluginTriggerBenchmark" />
			</feature>
		</config-file>

		<config-file parent="/*" target="AndroidManifest.xml"></config-file>
		<source-file src="src/android/cordovaPluginTriggerBenchmark.java" target-dir="src/cordova-plugin-trigger-benchmark/cordova-plugin-trigger-benchmark" />
	</platform>

	<!-- browser -->
    <platform name="browser">
        <js-module src="www/browser/cordovaPluginTriggerBenchmark.js" name="cordovaPluginTriggerBenchmark">
            <runs/>
            <clobbers target="cordova.plugins.cordovaPluginTriggerBenchmark" />
        </js-module>
    </platform>
</plugin>